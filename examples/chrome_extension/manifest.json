{
    "manifest_version": 3,
    "name": "Request Sentinel Chrome Extension",
    "description": "Request Interception using Request Sentinel",
    "version": "1.0",
    "action": {
      "default_popup": "request_sentinel.html",
      "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "interceptor.js"
    },
    "permissions": [
        "webRequest",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
  }